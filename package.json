{
  "name": "decaffeinate",
  "description": "Move your CoffeeScript source to modern JavaScript.",
  "version": "0.0.0-development",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "bin": {
    "decaffeinate": "./bin/decaffeinate"
  },
  "scripts": {
    "lint": "tslint --config tslint.json --project tsconfig.json --type-check",
    "lint-fix": "tslint --config tslint.json --project tsconfig.json --type-check --fix",
    "pretest": "yarn run build",
    "test": "mocha",
    "prebuild": "rimraf dist && mkdirp dist && npm run lint",
    "build": "script/build",
    "prepublish": "yarn run build",
    "update-website": "ts-node ./script/update-website.ts"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/decaffeinate/decaffeinate.git"
  },
  "keywords": [
    "coffee",
    "coffee-script",
    "convert",
    "es3",
    "es6",
    "esnext"
  ],
  "author": "Brian Donovan",
  "contributors": [
    "Alan Pierce <alangpierce@gmail.com>"
  ],
  "license": "MIT",
  "dependencies": {
    "@types/babel-core": "^6.7.14",
    "@types/babylon": "^6.16.1",
    "@types/fs-extra": "^3.0.3",
    "add-variable-declarations": "^3.0.5",
    "ast-processor-babylon-config": "^1.0.0",
    "automatic-semicolon-insertion": "^1.0.2",
    "babel-preset-es2015": "^6.24.1",
    "babylon": "^6.17.4",
    "coffee-lex": "^8.0.0",
    "decaffeinate-coffeescript": "1.10.0-patch26",
    "decaffeinate-parser": "^18.0.0",
    "detect-indent": "^4.0.0",
    "esnext": "^3.1.7",
    "lines-and-columns": "^1.1.5",
    "magic-string": "^0.22.1",
    "tslib": "^1.7.1"
  },
  "engines": {
    "node": ">=4"
  },
  "files": [
    "bin",
    "dist"
  ],
  "devDependencies": {
    "@types/mocha": "^2.2.41",
    "@types/mz": "0.0.31",
    "@types/node": "^8.0.0",
    "babel-core": "^6.25.0",
    "fs-extra": "^3.0.1",
    "mkdirp": "^0.5.1",
    "mocha": "^3.0.0",
    "mversion": "^1.10.1",
    "mz": "^2.6.0",
    "rimraf": "^2.5.4",
    "semantic-release": "^6.3.6",
    "surge": "^0.19.0",
    "ts-node": "^3.0.0",
    "tslint": "^5.5.0",
    "typescript": "^2.4.1"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "greenkeeper": {
    "ignore": [
      "repeating",
      "magic-string",
      "detect-indent"
    ]
  }
}
